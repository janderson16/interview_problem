<h1>List of Users</h1>
<!-- users table -->
<table class="users" border="1" width="100%" ID="Table2" style="margin: 0px;">
  <thead>
      <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
      </tr>
  </thead>
  <tbody id="users">
    <% @users.each do |user| %>
    <tr class="user-row user-row-<%= user.id %>" data-id="<%= user.id %>">
      <td><p><%= user.first_name%></p></td>
      <td><p><%= user.last_name %></a></p></td>
      <td><p><%= user.email %></p></td>
    </tr>
      <% end %>
  </tbody>
</table>
